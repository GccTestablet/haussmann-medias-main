{
  "name": "gccteam/haussmann-medias-main",
  "description": "Haussmann Medias - Main site",
  "type": "project",
  "license": "proprietary",
  "minimum-stability": "stable",
  "prefer-stable": true,
  "require": {
    "php": ">=8.2",
    "ext-ctype": "*",
    "ext-dom": "*",
    "ext-fileinfo": "*",
    "ext-iconv": "*",
    "ext-simplexml": "*",
    "doctrine/doctrine-bundle": "^2.11.1",
    "doctrine/doctrine-migrations-bundle": "^3.3",
    "doctrine/orm": "^2.17.1",
    "gedmo/doctrine-extensions": "^3.13",
    "knplabs/knp-paginator-bundle": "^6.3",
    "league/csv": "^9.11",
    "notfloran/mjml-bundle": "^3.7.1",
    "phing/phing": "^2.17.4",
    "phpdocumentor/reflection-docblock": "^5.3",
    "phpstan/phpdoc-parser": "^1.24.4",
    "sentry/sentry-symfony": "^4.12",
    "stof/doctrine-extensions-bundle": "^1.9",
    "symfony/asset": "6.3.*",
    "symfony/console": "6.3.*",
    "symfony/doctrine-messenger": "6.3.*",
    "symfony/dotenv": "6.3.*",
    "symfony/expression-language": "6.3.*",
    "symfony/filesystem": "6.3.*",
    "symfony/flex": "^2.4.1",
    "symfony/form": "6.3.*",
    "symfony/framework-bundle": "6.3.*",
    "symfony/http-client": "6.3.*",
    "symfony/intl": "6.3.*",
    "symfony/mailer": "6.3.*",
    "symfony/messenger": "6.3.*",
    "symfony/mime": "6.3.*",
    "symfony/monolog-bundle": "^3.10",
    "symfony/notifier": "6.3.*",
    "symfony/process": "6.3.*",
    "symfony/property-access": "6.3.*",
    "symfony/property-info": "6.3.*",
    "symfony/runtime": "6.3.*",
    "symfony/security-bundle": "6.3.*",
    "symfony/serializer": "6.3.*",
    "symfony/stimulus-bundle": "^2.13.2",
    "symfony/string": "6.3.*",
    "symfony/translation": "6.3.*",
    "symfony/twig-bundle": "6.3.*",
    "symfony/uid": "6.3.*",
    "symfony/ux-autocomplete": "^2.13.2",
    "symfony/ux-live-component": "^2.13.2",
    "symfony/ux-twig-component": "^2.13.1",
    "symfony/validator": "6.3.*",
    "symfony/web-link": "6.3.*",
    "symfony/webpack-encore-bundle": "^2.1.1",
    "symfony/yaml": "6.3.*",
    "twig/extra-bundle": "^3.8",
    "twig/intl-extra": "^3.8",
    "twig/twig": "^2.12|^3.8"
  },
  "config": {
    "bin-dir": "bin",
    "allow-plugins": {
      "php-http/discovery": true,
      "symfony/flex": true,
      "symfony/runtime": true,
      "phpstan/extension-installer": true
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "replace": {
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php72": "*",
    "symfony/polyfill-php73": "*",
    "symfony/polyfill-php74": "*",
    "symfony/polyfill-php80": "*",
    "symfony/polyfill-php81": "*"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "6.3.*"
    }
  },
  "require-dev": {
    "alexandresalome/mailcatcher": "^1.4",
    "behat/mink-selenium2-driver": "^1.6",
    "dbrekelmans/bdi": "^1.1",
    "doctrine/doctrine-fixtures-bundle": "^3.5.1",
    "friends-of-behat/mink": "^1.10",
    "friends-of-behat/mink-browserkit-driver": "^1.6.1",
    "friends-of-behat/mink-extension": "^2.7.4",
    "friends-of-behat/symfony-extension": "^2.4.1",
    "friendsofphp/php-cs-fixer": "^3.40",
    "genesis/behat-fail-aid": "^3.7.5",
    "jangregor/phpstan-prophecy": "^1.0",
    "kubawerlos/php-cs-fixer-custom-fixers": "^3.17",
    "phpspec/prophecy-phpunit": "^2.0",
    "phpstan/extension-installer": "^1.3",
    "phpstan/phpstan": "^1.10.45",
    "phpstan/phpstan-doctrine": "^1.3",
    "phpstan/phpstan-phpunit": "^1.3",
    "phpstan/phpstan-symfony": "^1.3.5",
    "phpunit/phpunit": "^9.6.13",
    "rector/rector": "^0.18.11",
    "symfony/browser-kit": "6.3.*",
    "symfony/css-selector": "6.3.*",
    "symfony/debug-bundle": "6.3.*",
    "symfony/maker-bundle": "^1.51.1",
    "symfony/phpunit-bridge": "^6.3.8",
    "symfony/stopwatch": "6.3.*",
    "symfony/web-profiler-bundle": "6.3.*"
  }
}
