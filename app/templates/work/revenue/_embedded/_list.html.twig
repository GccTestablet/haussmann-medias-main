<table class="table">
    <thead>
    <tr>
        <th>{% trans from 'work' %}Distributor{% endtrans %}</th>
        <th>{% trans from 'work' %}Canal{% endtrans %}</th>
        <th>{% trans from 'work' %}Starts at{% endtrans %}</th>
        <th>{% trans from 'work' %}Ends at{% endtrans %}</th>
        <th>{% trans from 'work' %}Revenue{% endtrans %}</th>
        <th>{% trans from 'misc' %}Actions{% endtrans %}</th>
    </tr>
    </thead>
    <tbody>
    {% for revenue in revenues %}
        <tr>
            <td>{{ revenue.distributor.name }}</td>
            <td>{{ revenue.channel.name }}</td>
            <td>{{ revenue.startsAt|date_format }}</td>
            <td>{{ revenue.endsAt|date_format }}</td>
            <td>{{ revenue.revenue }}</td>
            <td>
                <a href="{{ path('app_work_revenue_show', {'work': work.id, 'id': revenue.id}) }}" class="btn btn-primary">{% trans from 'misc' %}Show{% endtrans %}</a>
                <a href="{{ path('app_work_revenue_update', {'work': work.id, 'id': revenue.id}) }}" class="btn btn-warning">{% trans from 'misc' %}Update{% endtrans %}</a>
            </td>
        </tr>
    {% endfor %}
    </tbody>
</table>