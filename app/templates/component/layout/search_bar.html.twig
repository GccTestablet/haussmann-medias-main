<div {{ attributes }} >
    <input
            type="text"
            data-model="query"
            placeholder="Search an artwork, contract, beneficiary ..."
            class="form-control"
    >

    {% if this.results|length > 0 %}
        <div data-loading="addClass(opacity-50)" class="mt-3 row">
            {% for result in this.results %}
                {{ dump(result) }}
            {% endfor %}
        </div>
    {% else %}
        <div class="pt-3">Oh! No result found matching "{{ query }}"</div>
    {% endif %}
</div>
